<%= render "/layouts/refactoring/search_form", url: technique_category_other_techniques_path, q: @q %>
<%= render "/layouts/refactoring/technique_blank" %>
<table class="table<%= ' no_data_table' if @techniques.blank? %>">
  <%= render '/layouts/refactoring/show_thead' %>
  <tbody>
    <% @techniques.each_with_index do |technique, index| %>
      <tr>
        <th scope="row"><%= index+ 1 %></th>
        <td class="index_image"><%= render "/layouts/refactoring/no_image", anything: technique %></td>
        <td><%=link_to technique.name, other_technique_path(technique.id) %></td>
        <td><%=technique.cost %></td>
        <td><%=technique.created_at.strftime('%Y/%m/%d') %></td>
        <td><%=technique.updated_at.strftime('%Y/%m/%d') %></td>
      </tr>
      <% end%>
    </tbody>
</table>
