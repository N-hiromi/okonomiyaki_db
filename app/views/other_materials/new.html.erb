<h2 class="mb-4">その他材料登録</h2>
<div class="form">
  <table class="table">
    <%= form_with model: @other_material, url: {controller: 'other_materials', action: 'create'},local:true, id: 'other_material', method: :post do |f| %>
      <tbody>
        <%= render "/layouts/common_material_form", f: f %>
    <% end %>
  </table>
  <div class="text-right float-end">
    <button class="btn btn-primary" type="submit" form="other_material">登録</button>
  </div>
</div>

