<disv class="main users-edit users-right col-9">
  <div class="container col-6">
    <div class="form-heading">アカウント編集</div>
    <div class="form users-form">
      <div class="form-body">
        <% end %>
        <%= form_with model: current_user, local:true do |f| %>
        <div class"form-group">
            <label>名前</label>
              <%= f.text_field:name, class: "form-control" %>
        </div>
        <div class"form-group">
            <p>画像</p>
             <%= f.file_field:image, class: "form-control" %>
        </div>
        <div class"form-group">
            <label>部署</label>
              <%= f.select :faculty, [["経理部", "経理部"], ["開発部", "開発部"], ["営業部", "営業部"], ["人事部", "人事部"]], include_blank: "選択して下さい" %>
        </div>
          <div class"form-group">
                <%= f.submit "Save", class: "btn btn-primary" %>
          </div>
        <% end %>
      </div>
    </div>
  </div>
</div>