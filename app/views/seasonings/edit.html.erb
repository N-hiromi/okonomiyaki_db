<%= form_with model: @seasoning, url: {controller: 'seasonings', action: 'update'}, local:true, method: :patch do |f| %>
  <%= render "/layouts/refactoring/new_and_edit_table/seasoning", f: f%>
<% end %>
<div class="float-end">
  <%= button_to "削除", "/okonomiyakidb/seasonings/#{@seasoning.id}", data: { confirm: "削除してもよろしいですか?" }, method: :delete, class:"btn btn-danger mt-1" %>
</div>
